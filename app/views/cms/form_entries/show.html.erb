<% use_page_title "View Entry" %>

<%= render partial: 'buttons', layout: 'page_title' %>


<%= render layout: 'main_with_sidebar' do %>
    <dl>
      <% @entry.form.fields.each do |field| %>
          <dt><%= field.label %></dt>
          <dd><%= @entry.data_columns[field.name] %></dd>
      <% end %>
    </dl>
<% end %>